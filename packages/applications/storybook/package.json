{
  "name": "@applications/storybook",
  "description": "Deployable web application of Storybook",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "storybook build",
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "eject": "react-scripts eject",
    "format:check": "prettier --ignore-path ./.gitignore --check .",
    "format:repair": "prettier --ignore-path ./.gitignore --list-different --write .",
    "lint": "eslint ./.storybook ./src",
    "start": "storybook dev -p 6006"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@bashtex/basics": "0.0.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/preset-create-react-app": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-webpack5": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@config/eslint": "0.0.0",
    "@config/prettier": "0.0.0",
    "@config/typescript": "0.0.0",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.3.0",
    "eslint": "8.41.0",
    "eslint-plugin-storybook": "^0.8.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "sass": "^1.64.1",
    "storybook": "^8.1.3",
    "webpack": "^5.89.0"
  },
  "prettier": "@config/prettier"
}
