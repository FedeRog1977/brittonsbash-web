{
  "homepage": "http://brittonsbash.com",
  "name": "@applications/brittonsbash",
  "description": "Deployable web application of brittonsbash",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./build && rimraf ./node_modules",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format:check": "prettier --ignore-path ./.gitignore --check .",
    "format:repair": "prettier --ignore-path ./.gitignore --list-different --write .",
    "lint": "eslint ./src",
    "predeploy": "npm run build",
    "start": "react-scripts start"
  },
  "dependencies": {
    "@bashtex/basics": "0.0.0",
    "@utils/react": "0.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@config/eslint": "0.0.0",
    "@config/prettier": "0.0.0",
    "@config/typescript": "0.0.0",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.3.0",
    "eslint": "8.41.0",
    "gh-pages": "^5.0.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1"
  },
  "prettier": "@config/prettier",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
